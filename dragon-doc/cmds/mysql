1. select * from table_name into outfile "/fhdragon/data/test1"
2. mysql -ufhdragon -pmysql -p3360 -hx.x.x.x db_name -e"select * from table_name" > /fhdragon/data/test1
3. tee /fhdragon/data/test
   select * from table_name
   exit
4. LOAD DATA INFILE '/fhdragon/data/open data/twitter_rv_1y.txt' INTO TABLE user_followers FIELDS TERMINATED BY '\t' LINES  TERMINATED BY '\n';
5. UPDATE tablename SET  field = REPLACE(REPLACE(field, CHAR(10), ”), CHAR(13), ”);
    char(10):  换行符
    char(13):  回车符
