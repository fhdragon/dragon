1. create table user_followers( id bigint(64) unsigned not null, fid bigint(64) unsigned not null);
2. create table user_followers_pk(id bigint(64) unsigned not null comment 'user id', fid bigint(64) unsigned not null comment 'follower id');
   alter table user_followers_pk add primary key(id, fid);
